<p id="notice"><%= notice %></p>

<h1>Listing Inproceedings</h1>

<table>
  <thead>
    <tr>
      <th>Citation key</th>
      <th>Author</th>
      <th>Title</th>
      <th>Booktitle</th>
      <th>Year</th>
      <th>Editor</th>
      <th>Pages</th>
      <th>Organization</th>
      <th>Address</th>
      <th>Month</th>
      <th>Note</th>
      <th>Key</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inproceedings.each do |inproceeding| %>
      <tr>
        <td><%= inproceeding.citation_key %></td>
        <td><%= inproceeding.author %></td>
        <td><%= inproceeding.title %></td>
        <td><%= inproceeding.booktitle %></td>
        <td><%= inproceeding.year %></td>
        <td><%= inproceeding.editor %></td>
        <td><%= inproceeding.pages %></td>
        <td><%= inproceeding.organization %></td>
        <td><%= inproceeding.address %></td>
        <td><%= inproceeding.month %></td>
        <td><%= inproceeding.note %></td>
        <td><%= inproceeding.key %></td>
        <td><%= link_to 'Show', inproceeding %></td>
        <td><%= link_to 'Edit', edit_inproceeding_path(inproceeding) %></td>
        <td><%= link_to 'Destroy', inproceeding, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inproceeding', new_inproceeding_path %>
